   ],
   "source": [
    "evaluator = RegressionEvaluator(metricName=\"rmse\"\\\n",
    "                                ,labelCol=\"rating\"\\\n",
    "                                ,predictionCol=\"prediction\")\n",
    "rmse = evaluator.evaluate(predictions)\n",
    "print(\"Root-mean-square error = %f\" % rmse)"
   ]
  },
